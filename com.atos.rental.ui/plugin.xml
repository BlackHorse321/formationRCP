<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.atos.rental.ui.category1"
            name="name">
      </category>
      <view
            category="com.atos.rental.ui.category1"
            class="com.atos.rental.ui.views.RentalPropertyView"
            id="com.atos.rental.ui.rentalPropertyView"
            name="rentalPropertyView"
            restorable="true">
      </view>
      <view
            category="com.atos.rental.ui.category1"
            class="com.atos.rental.ui.views.RentalPropetertyTreeView"
            id="com.atos.rental.ui.view1"
            name="agencies"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.atos.rental.ui.perspectives.RentalPerspectives"
            icon="icons/Agency.png"
            id="com.atos.rental.ui.perspectives.RentalPerspectives"
            name="Rental">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.atos.rental.preference.RentalPreference"
            id="com.atos.rental.ui.preference"
            name="Rental">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.atos.rental.preference.AbstractPreferenceRentalInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="com.atos.rental.HWmenu"
               label="Hello World menu">
            <command
                  commandId="com.atos.rental.ui.HWcommandmenu"
                  label="Hello World Command"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.views.ResourceNavigator?after=additions">
         <command
               commandId="com.atos.rental.ui.HWcommandmenu"
               label="HW Navigator"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.atos.rental.ui.view1?after=additions">
         <command
               commandId="com.atos.rental.ui.HWcommandmenu"
               label="Hello World popup"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.atos.rental.ui.HWcommandmenu"
            name="my command">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.atos.rental.handler.SampleHandler"
            commandId="com.atos.rental.ui.HWcommandmenu">
      </handler>
   </extension>

</plugin>
