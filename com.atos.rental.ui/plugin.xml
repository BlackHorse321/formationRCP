<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="com.atos.ui.color.property" name="com.atos.ui.color.property" schema="schema/com.atos.ui.color.property.exsd"/>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.atos.rental.ui.category1"
            name="%category.name">
      </category>
      <view
            category="com.atos.rental.ui.category1"
            class="com.atos.rental.ui.views.RentalPropertyView"
            id="com.atos.rental.ui.rentalPropertyView"
            name="%view.name"
            restorable="true">
      </view>
      <view
            category="com.atos.rental.ui.category1"
            class="com.atos.rental.ui.views.RentalPropetertyTreeView"
            id="com.atos.rental.ui.view1"
            name="%view.name.0"
            restorable="true">
      </view>
      <view
            category="com.atos.rental.ui.category1"
            class="com.atos.rental.ui.views.CustomerPropertyView"
            id="com.atos.rental.ui.CustomerView"
            name="CustomerView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.atos.rental.ui.perspectives.RentalPerspectives"
            icon="icons/Agency.png"
            id="com.atos.rental.ui.perspectives.RentalPerspectives"
            name="%perspective.name">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.atos.rental.preference.RentalColorChoice"
            id="com.atos.rental.ui.RentalColor"
            name="Rental Palette">
      </page>
      <page
            category="com.atos.rental.ui.RentalColor"
            class="com.atos.rental.preference.RentalPreference"
            id="com.atos.rental.ui.preference"
            name="Default">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.atos.rental.preference.AbstractPreferenceRentalInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="com.atos.rental.HWmenu"
               label="%menu.label">
            <command
                  commandId="com.atos.rental.ui.HWcommandmenu"
                  label="%command.label"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.views.ResourceNavigator?after=additions">
         <command
               commandId="com.atos.rental.ui.HWcommandmenu"
               label="%command.label.0"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.atos.rental.ui.view1?after=additions">
         <command
               commandId="com.atos.rental.ui.HWcommandmenu"
               label="%command.label.1"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.atos.rental.ui.HWcommandmenu"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.atos.rental.handler.SampleHandler"
            commandId="com.atos.rental.ui.HWcommandmenu">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.atos.rental.ui.HWcommandmenu"
            contextId="org.eclipse.ui.contexts.dialogAndWindow"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+9">
      </key>
   </extension>
   <extension
         id="id4"
         name="name"
         point="com.atos.ui.color.property">
      <palette
            Class="com.atos.rental.preference.Daltonien"
            id="com.atos.rental.ui.Daltonien"
            name="Daltonien">
      </palette>
      <palette
            Class="com.atos.rental.preference.CRM"
            id="com.atos.rental.ui.crm"
            name="CRM">
      </palette>
      <palette
            Class="com.atos.rental.preference.DefaultPalette"
            id="com.atos.rental.ui.default.palette"
            name="Default">
      </palette>
   </extension>

</plugin>
